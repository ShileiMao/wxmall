<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.dao.StoreConfigDao">
    <select id="selectAllConfigs" resultType="com.example.entity.StoreConfig">
        SELECT * FROM store_config
    </select>

    <select id="selectConfigByKey" resultType="com.example.entity.StoreConfig">
        SELECT * FROM store_config
        where keyname = #{keyName}
    </select>

</mapper>
